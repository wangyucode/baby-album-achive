<!-- pages/my/support-operation/more-app/more-app.wxml -->
<view class='view-container'>
    <view class='my-item' hover-class='my-item-hover' bindtap="toRoll">
        <image class="image-logo" src='/assets/roll.jpg' />
        <view class="vbox">
            <text class="text-logo">3D滚蛋吧</text>
            <text class='my-item-desc'>一起来玩滚蛋吧，看谁滚的远。</text>
        </view>
        <view class='flex-open'></view>
        <image class='my-item-right-arrow' src='/assets/icons/arrow_down.png'></image>
    </view>
    <view class='dividing-line' />
    <view class='my-item' hover-class='my-item-hover' bindtap="toClipboard">
        <image class="image-logo" src='/assets/clipboard.jpg' />
        <view class="vbox">
            <text class="text-logo">跨平台剪切板</text>
            <text class='my-item-desc'>无需登录，跨平台，跨设备记录文字、网址。</text>
        </view>
        <view class='flex-open'></view>
        <image class='my-item-right-arrow' src='/assets/icons/arrow_down.png'></image>
    </view>
</view>